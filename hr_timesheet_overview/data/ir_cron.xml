<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">

    <record id="ir_cron_timesheet_hours_overview_update" model="ir.cron">
        <field name="name">HR Employee Hours Report: Update Data</field>
        <field name="model_id" ref="hr_timesheet_overview.model_hr_employee_hour" />
        <field name="active" eval="True" />
        <field name="priority" eval="0" />
        <field name="state">code</field>
        <field name="interval_type">minutes</field>
        <field name="interval_number">30</field>
        <field name="code">model.action_generate_data()</field>
        <field
            name="nextcall"
            eval="(DateTime.now() + relativedelta(minutes=30)).strftime('%Y-%m-%d 00:00:00')"
        />
    </record>

</odoo>
