<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
	    <record id="ir_rule_hr_contract_employee" model="ir.rule">
	        <field name="model_id" ref="hr_contract.model_hr_contract"/>
	        <field name="domain_force">[('employee_id.user_id','=',user.id)]</field>
	        <field name="name">Contract employee rights</field>
	        <field eval="[(6,0,[ref('base.group_user')])]" name="groups"/>
	        <field eval="0" name="perm_unlink"/>
	        <field eval="0" name="perm_write"/>
	        <field eval="1" name="perm_read"/>
	        <field eval="0" name="perm_create"/>
    	</record>
	    <record id="ir_rule_hr_contract_hr_manager" model="ir.rule">
	        <field name="model_id" ref="hr_contract.model_hr_contract"/>
	        <field name="domain_force">[(1,'=',1)]</field>
	        <field name="name">Contract hr manager rights</field>
	        <field eval="[(6,0,[ref('base.group_hr_manager')])]" name="groups"/>
	        <field eval="0" name="perm_unlink"/>
	        <field eval="0" name="perm_write"/>
	        <field eval="1" name="perm_read"/>
	        <field eval="0" name="perm_create"/>
    	</record>
    </data>
</openerp>