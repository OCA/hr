<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template
        id="assets_backend"
        name="hr_org_chart_overview assets"
        inherit_id="web.assets_backend"
    >
        <xpath expr="." position="inside">
            <script
                type="text/javascript"
                src="/hr_org_chart_overview/static/src/js/hr_org_chart_overview.js"
            />
            <script
                type="text/javascript"
                src="/hr_org_chart_overview/static/src/lib/orgchart/html2canvas.min.js"
            />
            <script
                type="text/javascript"
                src="/hr_org_chart_overview/static/src/lib/orgchart/jspdf.min.js"
            />
            <script
                type="text/javascript"
                src="/hr_org_chart_overview/static/src/lib/orgchart/jquery.orgchart.js"
            />
            <link
                rel="stylesheet"
                type="text/css"
                href="/hr_org_chart_overview/static/src/lib/orgchart/jquery.orgchart.css"
            />
            <link
                rel="stylesheet"
                type="text/scss"
                href="/hr_org_chart_overview/static/src/scss/hr_org_chart_style.scss"
            />
        </xpath>
    </template>
</odoo>
