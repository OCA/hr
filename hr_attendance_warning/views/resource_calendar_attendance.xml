<odoo>

    <record model="ir.ui.view" id="resource_calendar_attendance_tree_view">
        <field name="name">resource.calendar.attendance.tree (in hr_attendance_warning)</field>
        <field name="model">resource.calendar.attendance</field>
        <field name="inherit_id" ref="resource.view_resource_calendar_attendance_tree"/>
        <field name="arch" type="xml">
            <field name="date_to" position="after">
                <field name="margin_from"/>
                <field name="margin_to"/>
                <field name="next_check_from" group="base.group_no_one"/>
                <field name="next_check_to" group="base.group_no_one"/>
            </field>
        </field>
    </record>

</odoo>
